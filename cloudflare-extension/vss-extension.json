{
  "$schema": "http://json.schemastore.org/vss-extension",
  "manifestVersion": 1,
  "id": "tjosepo-cloudflare-extension",
  "name": "Cloudflare Deployment Extension",
  "version": "0.1.0",
  "publisher": "tjosepo",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "An Azure Pipelines Task Extension for projects using Cloudflare",
  "categories": [
    "Azure Pipelines"
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "icons": {
    "default": "icon.gif"
  },
  "files": [
    {
      "path": "CloudflarePagesComment/dist"
    },
    {
      "path": "CloudflarePagesComment/task.json"
    },
    {
      "path": "CloudflarePagesComment/icon.png"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "CloudflarePagesComment"
      }
    }
  ]
}